programa
{
	
	funcao inicio()
	{
		inteiro data_de_nascimento
		escreva("informe seu data de nascimento: ")
		leia(data_de_nascimento)

		data_de_nascimento = 2023 - data_de_nascimento

		escreva(data_de_nascimento)
	}
}

